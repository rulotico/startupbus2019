doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel="shortcut icon" type="image/png" href="/favicon.ico")
    link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.2/animate.min.css")
    
    meta(name="description" content="Startupbus en su decimo aniversario convoca a emprendedores, diseñadores, hustlers y programadores a iniciar un viaje de emprendimiento donde aprenderemos a crear e innovar empresas digitales. ") 
    meta(property="og:title" content="Startupbus México 2019")
    meta(property="og:description" content="Startupbus en su decimo aniversario convoca a emprendedores, diseñadores, hustlers y programadores a iniciar un viaje de emprendimiento donde aprenderemos a crear e innovar empresas digitales.")
    meta(property="og:site_name" content="Startupbus México 2019")
    meta(property="og:locale" content="es_MX")
    meta(property="og:type" content="website")
    meta(property="og:url" content="https://startupbus.herokuapp.com/")
